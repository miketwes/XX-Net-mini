[ req ]
prompt = no
distinguished_name = req_distinguished_name
req_extensions = v3_req
[ req_distinguished_name ]
C                      = CN
ST                     = Internet
L                      = Cernet
O                      = GoAgent-XX-Net-mini-4.5.2
OU                     = GoAgent-XX-Net-mini-4.5.2 Root
CN                     = GoAgent-XX-Net-mini-4.5.2 CA
emailAddress           = XXNet.mini@gmail.com
[ v3_req ]
basicConstraints = CA:TRUE, pathlen:0
extendedKeyUsage = serverAuth,emailProtection,timeStamping
keyUsage = keyCertSign, cRLSign
subjectKeyIdentifier = hash
